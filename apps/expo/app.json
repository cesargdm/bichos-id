{
	"expo": {
		"name": "Bichos ID",
		"slug": "bichos-id",
		"version": "1.0.0",
		"scheme": "bichos-id",
		"icon": "./assets/icon.png",
		"platforms": ["ios", "android"],
		"orientation": "portrait",
		"ios": {
			"googleServicesFile": "./GoogleService-Info.plist",
			"bundleIdentifier": "com.fucesa.bichos-id",
			"config": { "usesNonExemptEncryption": false }
		},
		"android": {
			"permissions": ["android.permission.CAMERA"],
			"googleServicesFile": "./google-services.json",
			"package": "com.fucesa.bichos_id"
		},
		"plugins": [
			"@react-native-firebase/app",
			"@react-native-firebase/auth",
			"@react-native-firebase/crashlytics",
			"expo-secure-store",
			["expo-build-properties", { "ios": { "useFrameworks": "static" } }],
			[
				"react-native-vision-camera",
				{
					"cameraPermissionText": "Esto permitirá que puedas tomar e identificar fotografías."
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "Esto permitirá que puedas seleccionar e identificar imágenes de tus fotos."
				}
			],
			[
				"@sentry/react-native/expo",
				{
					"url": "https://sentry.io/",
					"project": "bichos-id-native",
					"organization": "fucesa"
				}
			]
		],
		"extra": {
			"eas": {
				"projectId": "1a55a13f-ac76-44fa-b40f-10125fd7e4bf"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/1a55a13f-ac76-44fa-b40f-10125fd7e4bf",
			"fallbackToCacheTimeout": 3000
		}
	}
}
